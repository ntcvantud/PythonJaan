   "source": [
    "import numpy as np\n",
    "x = np.arange(-5, 5, 0.01)\n",
    "y = x ** 3\n",
    "\n",
    "# Đặt tên \n",
    "plt.title('Hàm y = x^3')\n",
    "plt.xlabel('Trục x')\n",
    "plt.ylabel('Trục y')\n",
    "\n",
    "# Hiển thị lưới \n",
    "plt.grid()\n",
    "\n",
    "# Vẽ plt.plot()\n",
    "plt.plot(x, y, label='y = x^3')\n",
    "\n",
    "# HIển thị chú thích \n",
    "plt.legend()\n",
    "\n",
    "# Hiển thị hình vẽ\n",
    "plt.show()\n"
